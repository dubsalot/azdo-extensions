{
    "manifestVersion": 1,
    "id": "<_EXTENSION_ID_>",
    "publisher": "WesGoodwin",
    "version": "<_EXTENSION_VERSION_>",
    "name": "<_EXTENSION_NAME_>",
    "description": "A hub to show an overall view of pipelines from source code to release notes.",
    "repository": {
        "type": "git",
        "uri": "https://dubsalot.visualstudio.com/Ease/_git/azdo-extensions?path=%2F&version=GB<_BRANCH_NAME_>&_a=contents"
    },
    "badges": [
        {
            "href": "https://dubsalot.visualstudio.com/Ease/_apis/build/status/Azure%20DevOps%20Extensions/azdo-extensions?branchName=<_BRANCH_NAME_>",
            "uri": "https://dubsalot.visualstudio.com/Ease/_apis/build/status/Azure%20DevOps%20Extensions/azdo-extensions?branchName=<_BRANCH_NAME_>",
            "description": "Pipeline Badge for Pipeline Overview"
        }
    ],       
    "categories": [
        "Azure Repos",
        "Azure Boards",
        "Azure Pipelines",
        "Azure Artifacts"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "<_EXTENSION_ICON_>"
    },
    "contributions": [
        {
            "id": "dubsalot-hub-groub",
            "type": "ms.vss-web.hub-group",
            "description": "Adds a 'Pipelions Overview' hub to the Work hub group.",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Pipeline Overview",
                "order": 99,
                "icon": "asset://static/<_EXTENSION_HUBGROUP_ICON_>"
            }
        }
    ],    
    "scopes": [
        "vso.code_full",
        "vso.build",
        "vso.build_execute",
        "vso.release_manage",
        "vso.project_manage",
        "vso.settings_write",
        "vso.work_full"
    ],    
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}

